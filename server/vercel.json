{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
    
  ],
  "routes": [
    {

      "src": "/Clients",
      "dest": "app.js"
    },
    {

      "src": "/Clients/(.*)",
      "dest": "app.js"
    },
    {

      "src": "/(.*)",
      "dest": "app.js"
    }, {

      "src": "/",
      "dest": "/build/index.html"
    }
    , {

      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}